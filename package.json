{
  "name": "weathersnap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lingui/core": "3.15.0",
    "@lingui/macro": "3.15.0",
    "@lingui/react": "3.15.0",
    "@reduxjs/toolkit": "^2.2.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.0",
    "react-scripts": "5.0.1",
    "rebass": "^4.0.7",
    "redux-localstorage-simple": "^2.5.1",
    "styled-components": "^6.1.12",
    "ua-parser-js": "^1.0.38"
  },
  "scripts": {
    "prei18n:extract": "touch src/locales/en-GB.po",
    "i18n:extract": "lingui extract --locale en-GB",
    "i18n:compile": "npm run i18n:extract && lingui compile",
    "i18n:pseudo": "lingui extract --locale pseudo && lingui compile",
    "prepare": "npm run i18n:compile",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@lingui/cli": "^3.15.0",
    "@types/lingui__core": "^2.7.1",
    "@types/lingui__macro": "^2.7.4",
    "@types/lingui__react": "^2.8.3",
    "@types/rebass": "^4.0.14",
    "@types/styled-components": "^5.1.34",
    "@types/ua-parser-js": "^0.7.39",
    "eslint-config-react-app": "^7.0.1",
    "touch-cli": "^0.0.1"
  }
}